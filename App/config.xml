<?xml version='1.0' encoding='utf-8'?>
<!-- para que funcione en android debe ser io.cordova.scheduleruwu-->
<widget id="SchedulerUwu" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SchedulerUwu</name>
    <description>Aplicacion creada con mucho amor por adri para llarita</description>
    <author href="https://upload.wikimedia.org/wikipedia/commons/thumb/f/f1/Heart_coraz%C3%B3n.svg/1200px-Heart_coraz%C3%B3n.svg.png">
        Adri
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="electron">
        <preference name="ElectronDebug" value="false" />
        <hook type="after_prepare" src="hooks/after_prepare/modify-main-js.js" />
        <preference name="ElectronSettingsFilePath" value="electron-settings.json" />
    </platform>
</widget>
